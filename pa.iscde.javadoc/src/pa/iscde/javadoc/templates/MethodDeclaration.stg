preVisit_MethodDeclaration(MethodDeclaration) ::= <<
\<hr> 
>>

postVisit_MethodDeclaration(MethodDeclaration) ::= <<
\<ul>
	\<ul>
		\<ul>\<b>Method Name:\</b>
			\<ul>\<b>\<a href="#<MethodDeclaration.startPosition>-<MethodDeclaration.length>"><MethodDeclaration.name>\</a>\</b>\</ul>
		\</ul>
		\<ul>\<b>Return Type:\</b>
			\<ul><MethodDeclaration.returnType2>\</ul>
		\</ul>		
		\<ul>\<b>Javadoc:\</b>
			\<ul>
				\<ul>\<span><MethodDeclaration.javadoc>\</span>\</ul> 			
 			\</ul>
		\</ul>		
		\<ul>\<b>Parameters:\</b>
			\<ul>
				\<ul><MethodDeclaration.parameters>\</ul>
				\<ul><length(MethodDeclaration.parameters)>\</ul>
 			\</ul>
		\</ul>
	\</ul>
\</ul>		
>>
